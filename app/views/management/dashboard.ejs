<script>
	$(document).ready(function () {
		$(".homeCard .border").hover(function () {
			$this = $(this)

			$this.find("img").css({
				"transition": "0.3s",
				"transform": "scale(0.7)"
			})

			setTimeout(function () {
				$this.find("img").css({
					"transition": "0.2s",
					"transform": "scale(1)"
				})
			}, 200)

		}, function () {
			$this.find("img").css({
				"transition": "0.2s",
				"transform": "scale(1)"
			})
		})


		let options = {
        dataLabels: {
            enabled: true
        },
        legend: {
            show: true
        },
        chart: {
            type: 'donut',
            height: 200
        },
        plotOptions: {
            pie: {
                donut: {
                    size: '40%'
                }
            }
        },
        series: [44, 55, 13, 33],
        labels: ['Apple', 'Mango', 'Orange', 'Watermelon']
    }

    var chart = new ApexCharts(document.querySelector("#chart1"), options);

    chart.render();
	})
</script>
