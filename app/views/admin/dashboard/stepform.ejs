        <!-- School Timing -->
        <form action="/admin/schoolTiming" method="post" class="step d-none" id="schoolTiming" data-step="8">
            <div class="modal-header table-card-header">
                <h5 class="modal-title" id="add-schoolTiming-modal-title">School Timing</h5>
                <button type="button" class="btn add-btn btn-sm" id="add-more-schoolTiming"><i class="fas fa-plus"></i> </button>
            </div>
            <div class="modal-body">
